<style>
</style>
<table class="calendarMonthTable">
    <thead>
        <tr>
            <th>Sunday</th>
            <th>Monday</th>
            <th>Tuesday</th>
            <th>Wednesday</th>
            <th>Thursday</th>
            <th>Friday</th>
            <th>Saturday</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 1; i <= 5; i++)
        {
            <tr>
                @for (int j = 1; j <= 7; j++)
                {
                    <td>@(i * j)</td>
                }
            </tr>
        }
    </tbody>
</table>

