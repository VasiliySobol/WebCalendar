@model IEnumerable<web_calendar.Models.DisplayEventViewModel>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeBegin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotificationCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CalendarId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CalendarName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Place)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeBegin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotificationCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CalendarId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CalendarName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
